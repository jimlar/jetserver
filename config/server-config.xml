<?xml version="1.0"?>

<jetserver>

  <!-- deployment manager settings -->
  <deploymanager dropzone="../dropzone" deploy-dir="../deployments"/>


  <!-- Web server settings -->
  <webserver>

    <!-- timeout is socket timeout in seconds -->
    <socket port="8080" timeout="10"/>

    <!-- keep alive time is in seconds -->
    <threads max="10" min="1" start="1" keep-alive-time="60"/>  

    <mime-types-file>mime.types</mime-types-file>
    <default-mime-type>text/plain</default-mime-type>

  </webserver>
</jetserver>

