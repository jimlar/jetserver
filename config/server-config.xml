<?xml version="1.0"?>

<jetserver>

  <!-- timeout is socket timeout in seconds -->
  <webserver port="8080" buffersize="1024" threads="50" timeout="10">

    <root>../html</root>

    <mime-types-file>mime.types</mime-types-file>
    <default-mime-type>text/plain</default-mime-type>

    <welcome-file-list>
      <welcome-file>index.html</welcome-file>
      <welcome-file>index.htm</welcome-file>
    </welcome-file-list>

    <cache max-file-size="50" time-to-live="2000" />

  </webserver>
</jetserver>

